{
  "name": "release-it",
  "version": "3.0.0-beta.0",
  "description": "Interactive release tool for Git repositories + npm packages. Bump, commit, tag, push, build, publish to npm. GitHub releases, upload assets. Distribution/component repositories.",
  "keywords": [
    "build",
    "commit",
    "distribution",
    "git",
    "github",
    "interactive",
    "npm",
    "publish",
    "push",
    "release",
    "repository",
    "script",
    "shell",
    "tag",
    "tool",
    "version"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:webpro/release-it.git"
  },
  "bin": "./bin/release.js",
  "main": "./lib/release.js",
  "scripts": {
    "lint": "eslint lib test",
    "prettier": "prettier --write \"{lib,test}/**/*.js\"",
    "test": "tape -r babel-register test/*.js",
    "coverage": "nyc --reporter=lcov npm test"
  },
  "author": {
    "email": "lars@webpro.nl",
    "name": "Lars Kappert"
  },
  "license": "MIT",
  "dependencies": {
    "@std/esm": "0.5.1",
    "async-retry": "1.1.3",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "babel-runtime": "6.26.0",
    "chalk": "2.1.0",
    "cpy": "5.1.0",
    "debug": "3.0.0",
    "github": "10.0.0",
    "glob": "7.1.2",
    "inquirer": "3.2.2",
    "lodash": "4.17.4",
    "ora": "1.3.0",
    "parse-repo": "1.0.1",
    "semver": "5.4.1",
    "shelljs": "0.7.8",
    "tmp-promise": "1.0.3",
    "yargs-parser": "7.0.0"
  },
  "devDependencies": {
    "eslint": "4.5.0",
    "eslint-config-prettier": "2.3.0",
    "eslint-plugin-prettier": "2.2.0",
    "nyc": "11.1.0",
    "prettier": "1.6.1",
    "proxyquire": "1.8.0",
    "tape": "4.8.0"
  },
  "engines": {
    "node": ">=4"
  },
  "@std/esm": {
    "esm": "js",
    "cjs": true
  }
}
